void InitBoard(char board[ROW][COL], int row, int col)
{
	int i, j;
	for (i = 0; i < row; i++)
	{
		for (j = 0; j < col; j++)
		{
			board[i][j] = ' ';
		}
	}
}
void DisplayBoard(char board[ROW][COL], int row, int col)
{
	int i, j;
	for (i = 0; i < row; i++)
	{
		if (i == 0)
		{
			printf("\n   ");
			for (j = 0; j < col; j++)
			{
				printf(" %d ", j + 1);
				if (j < col - 1)
					printf(" ");
				else
					printf("\n");
			}
		}
		printf(" %d ", i + 1);
		for (j = 0; j < col; j++)
		{
			printf(" %c ", board[i][j]);
			if (j < col - 1)
				printf("|");
			else
				printf("\n");
		}
		if (i < row - 1)
		{
			printf("   ");
			for (j = 0; j < col; j++)
			{
				printf("---");
				if (j < col - 1)
					printf("|");
				else
					printf("\n");
			}
		}
		else
			printf("\n");
	}
}
void PlayerMove(char board[ROW][COL], int row, int col)
{
	int x = 0, y = 0;
	printf("PlayerMove:>\n");
	DisplayBoard(board, ROW, COL);
	printf("Please enter the coordinates:>");
	while (1)
	{
		scanf("%d%d", &x, &y);
		if (x > 0 && x <= row && y > 0 && y <= col)
		{
			if (board[x - 1][y - 1] == ' ')
			{
				board[x - 1][y - 1] = '*';
				break;
			}
			else
			{
				printf("\nThe coordinate is already occupied!");
				printf("\nPlease enter the new coordinates:>");
			}
		}
		else
		{
			printf("\nCoordinates error!");
			printf("\nPlease enter the new coordinates:>");
		}
	}
}
